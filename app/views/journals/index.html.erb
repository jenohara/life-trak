<h1>Here are all your journals:</h1>

    <% @journals.each do |journal| %>
       <p> <%= link_to "#{journal.title} ", journal_path(journal) %> - <%= link_to "Edit", edit_journal_path(journal) %></p>
    <% end %>

<h2>Create a new journal:</h2>
<%= render partial:'form', locals:{journal: @journal} %>
