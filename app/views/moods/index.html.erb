<div class="main_body">
    <div>
        <h1>All your moods!</h1>
        <% @moods.each do |mood| %>
            <%= link_to mood.mood_type, mood_path(mood) %>           
            <hr>
        <% end %>
    </div>
    <h1>Add a new mood</h1>
    <%= render partial: 'new', locals: {object: @mood} %>
    <br>
</div>